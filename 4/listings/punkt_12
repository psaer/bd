DELETE FROM PRODUCT where PRODUCT_ID IN(
SELECT PRODUCT_ID FROM PRODUCT WHERE PRODUCT_ID NOT IN(SELECT PRODUCT_ID FROM review)
AND PRODUCT_ID NOT IN(select PRODUCT_ID from PRODUCT_AVALIABILITY)
AND PRODUCT_ID NOT IN(SELECT PRODUCT_ID from SUPPLY)
AND PRODUCT_ID NOT IN (SELECT PRODUCT_ID FROM SELLS_ENTRE)
AND PRODUCT_ID NOT IN(SELECT PRODUCT_ID FROM PRODUCT_TYPES));